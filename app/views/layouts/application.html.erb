<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag %>
    <%= render 'layouts/stylesheets' %>
    <%= javascript_include_tag :defaults %>
  </head>
  <body class="<%= @body %>">
     <%= link_to image_tag("melogo12.gif",:style =>"width:100%;", :alt =>"milkeggs"), root_path %>
     <%= link_to image_tag("melogo_words.gif",:style =>"width:100%;", :alt =>"milkeggs"), root_path %>
    <div id="header-wrap">
      <%= render 'layouts/header' %>
           <ul id="nav">
             <p class="welcome">
                <li><%= link_to "Home", home_path %></li>
            <li><%= link_to "Items", root_path %></li>
             <li><%= link_to "Stores", stores_path %></li>
              <li><%= link_to "Def.Items", defalut_items_path %></li>
              <li><%= link_to "Users", users_path %></li>
              <% if signed_in? %>
                  <% if false%>
                     <li><%= link_to "Retirees", retiree_path %></li>
                    <li> <%= link_to "Profile", current_user %> </li>
                    <li><%= link_to "Users", users_path %></li>
                    <li><%=link_to "Moderate Comments", comments_path  if current_user.admin? %></li>
                  <% end %>
                <li><%= link_to "Settings", edit_user_path(current_user) %></li>
            <% end %>
          </p>
          </ul>
        </nav>
        <% flash.each do |key, value| %>
          <div class="flash <%= key %>"><%= value %></div>
        <% end %>
        <%= yield %>
         <%= render 'layouts/footer' %>
          <br />
           <br />
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>
